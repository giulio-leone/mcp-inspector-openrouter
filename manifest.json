{
   "action": {},
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "js": [
            "content.js"
         ],
         "matches": [
            "\u003Call_urls>"
         ],
         "runAt": "document_start"
      }
   ],
   "description": "Inspect, monitor, and execute WebMCP tools manually or with OpenRouter",
   "host_permissions": [
      "\u003Call_urls>"
   ],
   "manifest_version": 3,
   "name": "Model Context Tool Inspector (OpenRouter)",
   "permissions": [
      "sidePanel",
      "activeTab",
      "scripting"
   ],
   "side_panel": {
      "default_path": "sidebar.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5"
}